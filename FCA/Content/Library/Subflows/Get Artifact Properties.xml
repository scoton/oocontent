  <scriptlet id="54314f74-57a7-4d8b-856c-ee86a5d41384">
    <annotation></annotation>
    <name>Operation Get Artifact Properties (b456af4a-885d-4889-968c-d4de53ae16d8)</name>
    <script>var propertyNameValuesList=scriptletContext.get(&quot;propertyNameValuesList&quot;);

try {
var pairs=propertyNameValuesList.split(&quot;\\|&quot;);
for (i=0; i&lt;pairs.length; i++)
{
    var variableName;
    var value;
    var pair=pairs[i].split(&quot;;&quot;);
    try
    {
        var variableName=pair[0];
        if(typeof(variableName) == &quot;undefined&quot;)
            continue;
    } catch(err)
    {
        continue;
    }
    scriptletContext.putGlobal(variableName,&quot;&quot;); //before this flow step, erase any possible existing value (before reading from propertyNameValuesList)
                                           // otherwise you may use the old value if the property was not created in CSA
    try
    {
        var value=pair[1];
        if(typeof(value) == &quot;undefined&quot;)
           value=&quot;&quot;;
    } catch(err)
    {
        value=&quot;&quot;;
    }
    scriptletContext.putGlobal(variableName,value);
}
} catch (errr)
{}
</script>
